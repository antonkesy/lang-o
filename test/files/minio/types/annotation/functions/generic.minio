-- "Pair(1, "hello")Pair("hello", 1)"
data Pair a b = Pair a b;

swap :: Pair a b -> Pair b a;
swap (Pair x y) = Pair y x;

-- showPair :: Show a => Show b => Pair a b -> String
showPair (Pair x y) = "Pair(" ++ show x ++ ", " ++ show y ++ ")";

p1 :: Pair Int String;
p1 = Pair 1 "hello";

main = do {
  putStr (showPair p1);
  putStr (showPair (swap p1));
};
