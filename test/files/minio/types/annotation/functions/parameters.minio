get :: Int;
get = 1;

add1 :: Int -> Int;
add1 x = x + 1;

add2 :: Int -> Int -> Int;
add2 x y = x + y;

add3 :: Int -> Int -> Int -> Int;
add3 x y z = x + y + z;

main = do {
  putStr (show get);
  putStr (show (add1 1));
  putStr (show (add1 2));
  putStr (show (add1 3));
  --
  putStr (show (add2 0 0));
  putStr (show (add2 1 1));
  putStr (show (add2 (add1 1) 2));
  --
  putStr (show (add3 0 0 0));
  putStr (show (add3 1 1 1));
  putStr (show (add3 (add2 1 2) 3 4))
};
