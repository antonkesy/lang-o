-- RUN: OK
-- TYPECHECK: OK
-- "42"hello""test"1.0"


inst first :: (a,b) -> a {
  first (x,y) = x;
};

inst second :: (a,b) -> b {
  second (x,y) = y;
};

main = do {
  let { p1 = (42, "hello"); p2 = ("test", 1.0) };
  putStr (show (first p1));
  putStr (show (second p1));
  putStr (show (first p2));
  putStr (show (second p2));
};
